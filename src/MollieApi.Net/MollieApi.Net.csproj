<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<RootNamespace>MollieApi.Net</RootNamespace>
		<AssemblyName>MollieApi.Net</AssemblyName>
		<TargetFrameworks>net451;netstandard2.0</TargetFrameworks>
		<TargetFramework>net451</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>MollieApi</PackageId>
		<Version>2.0.0</Version>
		<Authors>Rob Janssen</Authors>
		<Company />
		<Description>A C#/.net wrapper for the Mollie API version 2</Description>
		<Copyright>Copyright Rob Janssen 2020</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/janssenr/MollieApi.Net</PackageProjectUrl>
		<PackageIcon>favicon-1.png</PackageIcon>
		<PackageTags>mollie api rest data</PackageTags>
		<PackageReleaseNotes></PackageReleaseNotes>
		<AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
	</PropertyGroup>
	<ItemGroup Condition="'$(TargetFramework)' == 'net451'">
		<Reference Include="System.Net.Http" />
	</ItemGroup>
	<ItemGroup>
		<None Include="..\..\nuget\Package\images\favicon-1.png">
			<Pack>True</Pack>
			<PackagePath/>
		</None>
	</ItemGroup>
</Project>